//Client sends this when Posting to server via addPoint()
{
    red: value,
    green: value,
    blue: value,
    yellow: value,
    username: value,
}

//Client sends this when Posting to server via login()
{
    username: value
}






//Client receives this after each Post, for both addPoint() and login()
{
    users: [{username: value, clicks: value}],
    red: value,
    green: value,
    blue: value,
    yellow: value
}